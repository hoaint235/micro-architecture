{
  "name": "@container/root-config",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint src --ext js,ts,tsx",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "build": "yarn build:root && yarn build:setup",
    "build:root": "webpack build --config ./webpack.config.js",
    "build:setup": "webpack build --config ./webpack.importmap.js",
    "start": "concurrently \"yarn start:setup\" \"yarn start:root\" ",
    "start:root": "webpack serve --config ./webpack.config.dev.js",
    "start:setup": "webpack serve --config ./webpack.importmap.dev.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently yarn:test yarn:lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-ts-important-stuff": "^1.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "serve": "^11.3.2",
    "ts-config-single-spa": "^2.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0",
    "webpack-config-single-spa-ts": "^2.0.0",
    "webpack-dev-server": "^4.0.0-beta.0",
    "webpack-merge": "^5.5.0"
  },
  "dependencies": {
    "@types/jest": "^26.0.16",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.16.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "single-spa": "^5.8.2",
    "style-loader": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
